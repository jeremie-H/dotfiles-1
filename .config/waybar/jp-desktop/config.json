{
    "layer": "top",
    "modules-left": [
        "sway/workspaces"
    ],
    "modules-right": [
        "custom/mouse-bat",
        "custom/df-root",
        "custom/df-home",
        "custom/df-daten",
        "network#ethernet",
        "pulseaudio",
        "custom/quodlibet-volume",
        "clock#date",
        "clock#clock"
    ],
    "sway/workspaces": {
        "format": "{name}"
    },
    "custom/mouse-bat": {
        "exec": "~/.config/waybar/modules/mouse-bat.sh",
        "return-type": "json",
        "interval": 60,
        "format": " {icon}",
        "format-icons": [
            "",
            "",
            "",
            "",
            "",
            ""
        ]
    },
    "custom/df-root": {
        "exec": "~/.config/waybar/modules/df.sh /",
        "interval": 60,
        "format": " / {}"
    },
    "custom/df-home": {
        "exec": "~/.config/waybar/modules/df.sh /home/jplatte",
        "interval": 60,
        "format": "  {}"
    },
    "custom/df-daten": {
        "exec": "~/.config/waybar/modules/df-btrfs.sh /mnt/daten",
        "interval": 60,
        "format": " {}"
    },
    "network#ethernet": {
        "interface": "enp3s0",
        "format-ethernet": " {ipaddr}/{cidr}",
        "format-disconnected": " Disconnected"
    },
    "pulseaudio": {
        "format": "{icon} {volume} %",
        "format-bluetooth": "{icon} {volume} %",
        "format-muted": "",
        "format-icons": {
            "headphones": "",
            "handsfree": "",
            "headset": "",
            "phone": "",
            "portable": "",
            "car": "",
            "default": [
                "",
                ""
            ]
        },
        "on-click": "pavucontrol"
    },
    "custom/quodlibet-volume": {
        "exec": "~/.config/waybar/modules/quodlibet-volume.sh",
        "interval": 60,
        "format": " {} %"
    },
    "clock#date": {
        "format": " {:%d.%m}"
    },
    "clock#clock": {
        "format": " {:%H∶%M}"
    }
}